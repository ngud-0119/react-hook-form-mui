{
  "version": "1.1.3",
  "license": "MIT",
  "repository": "https://github.com/dohomi/react-hook-form-mui",
  "homepage": "https://react-hook-form-material-ui.now.sh",
  "keywords": [
    "react",
    "mui",
    "material-ui",
    "react-hook-form"
  ],
  "name": "react-form-hook-mui",
  "author": "Dominic Garms",
  "source": "src/index.ts",
  "main": "dist/react-form-hook-mui.cjs.js",
  "module": "dist/react-form-hook-mui.esm.js",
  "typings": "dist/react-form-hook-mui.cjs.d.ts",
  "files": [
    "dist",
    "TextFieldElement ",
    "FormContainer",
    "DatePickerElement",
    "CheckboxElement",
    "SelectElement",
    "CheckboxButtonGroup",
    "DateFnsProvider",
    "MultiSelectElement",
    "PasswordElement",
    "RadioButtonGroup",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check-types": "tsc --noemit",
    "deploy": "npm run build-storybook && cd storybook-static && vercel --name react-hook-form-material-ui --prod",
    "dev": "preconstruct dev",
    "watch": "preconstruct watch",
    "fix": "preconstruct fix",
    "build": "preconstruct build"
  },
  "peerDependencies": {
    "@date-io/date-fns": "> 1.3.x <2",
    "@material-ui/core": "> 4.7 <5",
    "@material-ui/icons": "> 4.7 <5",
    "@material-ui/pickers": "> 3.2.x <4",
    "date-fns": ">2.7 <3",
    "react": ">=17",
    "react-hook-form": ">=7.3.5",
    "typescript": ">= 4.1"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "TextFieldElement.tsx",
      "FormContainer.tsx",
      "DatePickerElement.tsx",
      "CheckboxElement.tsx",
      "SelectElement.tsx",
      "CheckboxButtonGroup.tsx",
      "DateFnsProvider.tsx",
      "MultiSelectElement.tsx",
      "PasswordElement.tsx",
      "RadioButtonGroup.tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-typescript": "^7.13.0",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@preconstruct/cli": "^2.1.0",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-docs": "6.2.9",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addons": "6.2.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "@storybook/react": "6.2.9",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "babel-loader": "^8.1.0",
    "date-fns": "^2.15.0",
    "mutation-observer": "^1.0.3",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.6.4",
    "react-is": "^17.0.1",
    "ts-loader": "^9.2.1",
    "tslib": "^2.0.3",
    "typescript": "^4.2.4"
  }
}
